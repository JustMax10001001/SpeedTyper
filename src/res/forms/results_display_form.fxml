<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.justsoft.speedtyper.ui.controllers.ResultsDisplayController">
    <LineChart layoutX="15.0" layoutY="131.0" prefHeight="256.0" prefWidth="568.0" AnchorPane.bottomAnchor="16.0"
               AnchorPane.leftAnchor="16.0" AnchorPane.rightAnchor="16.0" AnchorPane.topAnchor="128.0"
               fx:id="cpmChart">
        <xAxis>
            <CategoryAxis label="Date" side="BOTTOM"/>
        </xAxis>
        <yAxis>
            <NumberAxis label="CPM" side="LEFT"/>
        </yAxis>
    </LineChart>
    <HBox alignment="CENTER_LEFT" layoutX="16.0" layoutY="14.0" prefWidth="568.0" spacing="8.0"
          AnchorPane.leftAnchor="16.0" AnchorPane.rightAnchor="16.0" AnchorPane.topAnchor="18.0">
        <Label alignment="CENTER" prefWidth="81.0" text="Plot data since "/>
        <DatePicker fx:id="dateSincePicker"/>
        <Label text="up to"/>
        <DatePicker fx:id="dateUpToPicker"/>
    </HBox>
</AnchorPane>
